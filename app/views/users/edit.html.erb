<div class="test center">
  <h1>Profile</h1>

  <%= form_for @user do |form| %>
    <%= render 'shared/error_messages', :obj => @user %>
    <%= render partial: 'form', object: form %>

    <div>
      <%= form.submit "Save", :class => "button expand" %>
      <%= link_to 'Cancel', :back %>
    </div>
  <% end %>

</div>
