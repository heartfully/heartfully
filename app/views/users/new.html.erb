<div class="sign-up">
  <h2>Sign Up</h2>

  <%= render 'shared/error_messages', :obj => @user %>

  <%= form_for @user do |form| %>
    <%= render :partial => '/users/form', :object => form %>

    <div class="submit-field">
      <%= form.submit %>
    </div>

    <div class="other-links">
      <%= link_to "Sign In", sign_in_path %>
    </div>
  <% end %>
</div>
