<div class="test center">
  <h2 class="c-how__title center subhed purple mb">Sign Up</h2>


  <%= form_for @user do |form| %>

    <%= render 'shared/error_messages', :obj => @user %>
    <%= render :partial => '/users/form', :object => form %>

    <div>
      <%= form.submit "Sign Up", :class => "button expand" %>
    </div>
  <% end %>
  <p class="purple">Already have an account? <%= link_to "Log in", sign_in_path %></p>
</div>
