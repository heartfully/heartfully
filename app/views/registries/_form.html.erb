<%= form_for @registry, :url => registry_path(@registry) do |f| %>

  <%= render 'shared/error_messages', :obj => @registry %>

  <div class="text-field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="text-field">
    <%= f.label :url_slug %>
    <%= f.text_field :url_slug %>
  </div>

  <div class="text-field">
    <%= f.label :banner_img %>
    <%= f.text_field :banner_img %>
  </div>

  <div class="text-field">
    <%= f.label :profile_img %>
    <%= f.text_field :profile_img %>
  </div>

  <div class="text-field">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div class="text-field">
    <%= f.label :address_1 %>
    <%= f.text_field :address_1 %>
  </div>

  <div class="text-field">
    <%= f.label :address_2 %>
    <%= f.text_field :address_2 %>
  </div>

  <div class="text-field">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div class="text-field">
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>

  <div class="text-field">
    <%= f.label :postal_code %>
    <%= f.text_field :postal_code %>
  </div>

  <div class="text-field">
    <%= f.label :country %>
    <%= f.text_field :country %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
